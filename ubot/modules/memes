from ubot import *

__MODULE__ = "Memes"
__HELP__ = """
 <b>『 Bantuan untuk memes 』</b>

  <b>• Perintah:</b> <code>{0}memes [teks]</code>
  <b>• Penjelasan:</b> Untuk membuat kata memes random.
"""


@PY.UBOT("mms|memes", SUDO=True)
async def _(client, message):
    await memes_cmd(client, message)
